<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.musikfest"
    android:versionCode="3"
    android:versionName="1" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.musikfest.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="com.musikfest.SpillestederActivity" />
        <activity android:name="com.musikfest.ProgramActivity" />
        <activity android:name="com.musikfest.SpillestedInfoActivity" />
        <activity android:name="com.musikfest.BandInfoActivity" />
        <activity android:name="com.musikfest.BandActivity" />
        <activity android:name="com.musikfest.BandConcertsActivity" />
        <activity android:name="com.musikfest.ChooseBandFragment" />
        <activity android:name="com.musikfest.InfoActivity" />
        
        
    </application>
   
</manifest>
